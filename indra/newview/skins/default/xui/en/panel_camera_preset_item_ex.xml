<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 follows="all"
 height="25"
 layout="topleft"
 left="0"
 name="panel_camera_preset_item"
 top="0"
 width="300">
  <icon
   follows="all"
   bottom="-1"
   image_name="ListItem_Select"
   layout="topleft"
   left="0"
   name="selected_icon"
   right="-1"
   top="0"
   visible="false"
   />
  <icon
   follows="all"
   bottom="-1"
   image_name="ListItem_Over"
   layout="topleft"
   left="0"
   name="hovered_icon"
   right="-1"
   top="0"
   visible="false"
   />

  <icon
   follows="top|left"
   height="20"
   image_name="Cam_Preset_Side_Off"
   layout="topleft"
   left="7"
   mouse_opaque="false"
   name="preset_icon"
   tab_stop="false"
   top="2"
   width="20"
   />

  <text
   follows="top|left|right"
   font="SansSerifSmall"
   height="15"
   layout="topleft"
   left_pad="5"
   name="preset_name"
   parse_urls="false"
   right="-5"
   top="7"
   use_ellipses="true"
   >
    Text
  </text>

  <button
   follows="top|right"
   height="20"
   image_selected="Refresh_Off"
   image_pressed="Refresh_Off"
   image_unselected="Refresh_Off"
   layout="topleft"
   name="reset_btn"
   right="-5"
   tool_tip="Reset preset to default"
   top_delta="-3"
   visible="false"
   width="20"
   >
  </button>
  <button
   follows="right"
   height="20"
   image_selected="TrashItem_Off"
   image_pressed="TrashItem_Off"
   image_unselected="TrashItem_Off"
   layout="topleft"
   right="-5"
   name="delete_btn"
   tool_tip="Delete preset"
   top_delta="0"
   visible="false"
   width="20"
   >
  </button>
</panel>