<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
 border="false"
 can_close="true"
 can_minimize="true"
 can_resize="false"
 height="330"
 name="floater_chat_sounds"
 positioning="centered"
 title="Chat Sounds"
 width="510">

    <text
     follows="left|top"
     font.style="BOLD"
     height="15"
     layout="topleft"
     left="155"
     length="1"
     name="text_column_conv"
     top="12"
     width="165"
     type="string">
      New conversation
    </text>
    <text
     follows="left|top"
     font.style="BOLD"
     height="15"
     layout="topleft"
     left_pad="15"
     length="1"
     name="text_column_im"
     top_delta="0"
     width="165"
     type="string">
      Incoming message
    </text>

    <button
     control_name="PlaySoundNearbyChatIM"
     follows="top|left"
     height="18"
     image_selected="Audio_Off"
     image_unselected="AudioMute_Off"
     is_toggle="true"
     layout="topleft"
     left="15"
     name="sound_nearby_mute"
     tab_stop="false"
     top_pad="4"
     width="18" />
    <text
     height="16"
     layout="topleft"
     left_pad="7"
     name="sound_nearby_text"
     width="105"
     valign="center"
     top_delta="0">
      Nearby chat :
    </text>
    <combo_box
     enabled_control="PlaySoundNearbyChatIM"
     height="23"
     layout="topleft"
     left_pad="180"
     name="sound_nearby"
     top_delta="-1"
     width="150">
        <init_callback
         function="InitSoundsCombo"
         parameter="UISndEventChatAgent" />
    </combo_box>
    <button
     enabled_control="PlaySoundNearbyChatIM"
     follows="top|left"
     height="20"
     image_unselected="Play_Off"
     image_pressed="Play_Press"
     is_toggle="false"
     left_pad="1"
     top_delta="1"
     name="sound_nearby_preview"
     sound_flags="0"
     tool_tip="Preview the selected sound"
     width="20" >
    </button>
    <sound_drop_target
     height="23"
     layout="topleft"
     left="325"
     name="sound_nearby_drop"
     top_delta="0"
     width="175" />

