<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
 border="false"
 can_close="true"
 can_minimize="true"
 can_resize="false"
 height="335"
 name="floater_chat_sounds"
 positioning="centered"
 title="Chat Sounds"
 width="420">

    <text
     follows="left|top"
     font.style="BOLD"
     height="15"
     layout="topleft"
     left="15"
     length="1"
     name="chat_sounds"
     top="10"
     width="150"
     type="string">
      Chat sounds
    </text>

    <button
     control_name="PlaySoundFriendIM"
     follows="top|left"
     height="18"
     image_selected="Audio_Off"
     image_unselected="AudioMute_Off"
     is_toggle="true"
     layout="topleft"
     left="30"
     name="sound_im_friend_mute"
     tab_stop="false"
     top_pad="3"
     width="18" />
    <text
     height="16"
     layout="topleft"
     left_pad="7"
     name="im_friend"
     width="105"
     valign="center"
     top_delta="0">
      Friend IMs :
    </text>
    <combo_box
     enabled_control="PlaySoundFriendIM"
     height="23"
     layout="topleft"
     left_pad="5"
     name="sound_im_friend"
     top_delta="-2"
     width="225">
        <init_callback
         function="InitSoundsCombo"
         parameter="UISndEventIMFriend" />
    </combo_box>
    <button
     enabled_control="PlaySoundFriendIM"
     follows="top|left"
     height="20"
     image_unselected="Play_Off"
     image_pressed="Play_Press"
     is_toggle="false"
     left_pad="1"
     top_delta="1"
     name="sound_im_friend_preview"
     tool_tip="Preview the selected sound"
     width="20" >
    </button>
    <sound_drop_target
     height="23"
     layout="topleft"
     left="30"
     name="sound_im_friend_drop"
     top_delta="0"
     width="385" />

