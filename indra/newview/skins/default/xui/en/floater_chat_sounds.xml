<floater
 border="false"
 can_close="true"
 can_minimize="true"
 can_resize="false"
 height="335"
 name="floater_chat_sounds"
 positioning="centered"
 title="Chat Sounds"
 width="575">

    <text
     follows="left|top"
     font.style="BOLD"
     height="15"
     layout="topleft"
     left="165"
     length="1"
     name="text_column_conv"
     top_pad="10"
     width="165"
     type="string">
      New conversation sound
    </text>
    <text
     follows="left|top"
     font.style="BOLD"
     height="15"
     layout="topleft"
     left_pad="40"
     length="1"
     name="text_column_im"
     top_delta="0"
     width="165"
     type="string">
      Incoming message sound
    </text>

    <button
     control_name="PlaySoundFriendIM"
     follows="top|left"
     height="18"
     image_selected="Audio_Off"
     image_unselected="AudioMute_Off"
     is_toggle="true"
     layout="topleft"
     left="30"
     name="sound_friend_mute"
     tab_stop="false"
     top_pad="3"
     width="18" />
    <text
     height="16"
     layout="topleft"
     left_pad="7"
     name="sound_friend_text"
     width="105"
     valign="center"
     top_delta="0">
      Friend IMs :
    </text>
    <combo_box
     enabled_control="PlaySoundFriendIM"
     height="23"
     layout="topleft"
     left_pad="5"
     name="sound_friend_conv"
     top_delta="-2"
     width="175">
        <init_callback
         function="InitSoundsCombo"
         parameter="UISndEventConvFriend" />
    </combo_box>
    <button
     enabled_control="PlaySoundFriendIM"
     follows="top|left"
     height="20"
     image_unselected="Play_Off"
     image_pressed="Play_Press"
     is_toggle="false"
     left_pad="1"
     top_delta="1"
     name="sound_friend_conv_preview"
     tool_tip="Preview the selected sound"
     width="20" >
    </button>
    <sound_drop_target
     height="23"
     layout="topleft"
     left="30"
     name="sound_friend_conv_drop"
     top_delta="0"
     width="335" />
