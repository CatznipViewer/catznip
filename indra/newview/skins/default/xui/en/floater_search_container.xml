<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<multi_floater
 can_close="true"  
 can_minimize="true"
 can_resize="true"
 height="390"
 layout="topleft"
 name="floater_search_container"
 save_rect="true"
 save_visibility="true"
 single_instance="true"
 title="Second Life Search"
 width="396">
    <tab_container
     follows="left|right|top|bottom"
     height="390"
     layout="topleft"
     left="1"
     name="search_container"
     tab_position="top"
     tab_width="64"
     tab_min_width = "150"
     tab_height="30"
     use_custom_icon_ctrl="true"
     tab_icon_ctrl_pad="2"
     halign="center"
     use_ellipses="true"
     top="0"
     width="394">
      <first_tab
       tab_bottom_image_flash="Toolbar_Left_Flash"/>
      <middle_tab
       tab_bottom_image_flash="Toolbar_Middle_Flash"/>
      <last_tab
       tab_bottom_image_flash="Toolbar_Right_Flash"/>
    </tab_container>
</multi_floater>
