<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
 legacy_header_height="18"
 can_resize="true"
 height="400"
 layout="topleft"
 name="queue"
 help_topic="queue"
 save_rect="true"
 title="RESET PROGRESS"
 width="300">
    <floater.string
     name="Starting">
        Starting [START] of [COUNT] items.
    </floater.string>
    <floater.string
     name="Done">
        Done.
    </floater.string>
    <floater.string
     name="Resetting">
        Resetting
    </floater.string>
    <floater.string
     name="Running">
        Running
    </floater.string>
    <floater.string
     name="NotRunning">
        Not running
    </floater.string>

    <scroll_list
     follows="left|top|right|bottom"
     height="344"
     layout="topleft"
     left="6"
     name="queue output"
     top="20"
     width="290" />    
    <panel
     background_visible="false"
     follows="left|right|bottom"
     height="25"
     label="bottom_panel"
     layout="topleft"
     left="5"
     name="bottom_panel"
     top_pad="4" >
        <icon
         follows="left|right"
         height="25" 
         image_name="Toolbar_Left_Off"
         left="0"
         name="dummy_icon"
         top="1"
         width="215" />
        <button
         follows="right"
         height="25"
         image_hover_unselected="Toolbar_Right_Over"
         image_selected="Toolbar_Right_Selected"
         image_unselected="Toolbar_Right_Off"
         label="Close"
         left_pad="1"
         name="close"
         top="1"
         width="74" />
    </panel>     
</floater>
