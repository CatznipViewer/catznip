<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
  legacy_header_height="18"
  can_resize="true"
  height="300"
  layout="topleft"
  min_height="100"
  min_width="250"
  name="floater_web_content"
  help_topic="floater_web_content"
  save_rect="true"
  title=""
  initial_mime_type="text/html"
  width="450">
  <layout_stack
    bottom="-5"
    height="24"
    follows="left|right|top|bottom"
    layout="topleft"
    left="5"
    animate="false"
    name="stack1"
    orientation="vertical"
    top="16"
    right="-5">
    

    
    
    <layout_panel
      auto_resize="false"
      default_tab_group="1"
      follows="top|left|right"
      height="26"
      layout="topleft"
      left="0"
      min_height="27"
      name="nav_controls"
      right="-5">
           <button
     follows="top|left"
     height="24"
     image_hover_unselected="Toolbar_Middle_Over"
     image_overlay="Arrow_Left"
     image_overlay_alignment="center"
     image_selected="Toolbar_Middle_Selected"
     image_unselected="Toolbar_Middle_Off"
     tool_tip="Back"
     layout="topleft"
     name="back"
     width="28">
        <button.commit_callback
          function="WebContent.Back" />
      </button>
    <button
     follows="top|left"
     height="24"
     image_hover_unselected="Toolbar_Middle_Over"
     image_overlay="Arrow_Right"
     image_overlay_alignment="center"
     image_selected="Toolbar_Middle_Selected"
     image_unselected="Toolbar_Middle_Off"
     tool_tip="Forward"
     layout="topleft"
     left_pad="1"
     name="forward"
     top_delta="0"
     width="28">
        <button.commit_callback
          function="WebContent.Forward" />
      </button>
    <button
     follows="top|left"
     height="24"
     image_hover_unselected="Toolbar_Middle_Over"
     image_overlay="StopReload_Off"
     image_overlay_alignment="center"
     image_selected="Toolbar_Middle_Selected"
     image_unselected="Toolbar_Middle_Off"
     tool_tip="Stop navigation"
     layout="topleft"
     left="58"
     name="stop"
     top_delta="0"
     width="28">
        <button.commit_callback
          function="WebContent.Stop" />
      </button>
    <button
     follows="top|left"
     height="24"
     image_hover_unselected="Toolbar_Middle_Over"
     image_overlay="Refresh_Off"
     image_overlay_alignment="center"
     image_selected="Toolbar_Middle_Selected"
     image_unselected="Toolbar_Middle_Off"
     tool_tip="Reload page"
     layout="topleft"
     left="58"
     name="reload"
     top_delta="0"
     visible="false" 
     width="28">
        <button.commit_callback
          function="WebContent.Reload" />
      </button>

      <combo_box
        allow_text_entry="true"
        follows="left|top|right"
        tab_group="1"
        height="22"
        layout="topleft"
        left_pad="2"
        max_chars="1024"
        name="address"
        right="-30"
        combo_editor.select_on_focus="true"
        tool_tip="Enter URL here"
        top_delta="0">
        <combo_box.commit_callback
          function="WebContent.EnterAddress" />
      </combo_box>
      <icon
        name="media_secure_lock_flag"
        height="16"
        follows="top|left"
        image_name="Lock2"
        layout="topleft"
        left_delta="2"
        top_delta="2"
        visible="false" 
        tool_tip="Secured Browsing"
        width="16" />
           <button
     follows="top|right"
     height="24"
     image_hover_unselected="Toolbar_Middle_Over"
     image_overlay="ExternalBrowser_Off"
     image_overlay_alignment="center"
     image_selected="Toolbar_Middle_Selected"
     image_unselected="Toolbar_Middle_Off"
     tool_tip="Open URL in your desktop browser"
     top_delta="-2"
     layout="topleft"
    right="-1"
     name="popexternal"
     width="28">
        <button.commit_callback
          function="WebContent.PopExternal" />
      </button>

    </layout_panel>
    
    <layout_panel
      height="40"
      layout="topleft"
      left_delta="0"
      name="external_controls"
      top_delta="0"
      auto_resize="true"
      width="585">
      <web_browser
        bottom="60"
        follows="all"
        layout="topleft"
        left="0"
        name="webbrowser"
        top="0"/>
    </layout_panel>
        <layout_panel name="status_bar" 
                  height="12"
                  auto_resize="false">
      <text
        type="string"
        length="200"
        follows="bottom|left"
        height="20"
        layout="topleft"
        left="12"
        name="statusbartext"
        parse_urls="false"
        text_color="0.4 0.4 0.4 1"
        top_pad="-1"
        width="495"/>
      <progress_bar
        color_bar="0.3 1.0 0.3 1"
        follows="top|left|right"
        height="26"
        top="0"
        left="1"
        right="-1"
        layout="topleft"
        name="statusbarprogress"
        width="256"/>
    </layout_panel>
  </layout_stack>
</floater>
