<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 background_visible="true"
 bg_alpha_color="DkGray"
 follows="all"
 height="430"
 layout="topleft"
 left="0"
 name="Wearing"
 top="0"
 width="312">
    <wearing_items_list
     follows="all"
     height="400"
     layout="topleft"
     left="3"
     multi_select="true"
     name="cof_items_list"
     standalone="false"
     top="0"
     width="309"
     worn_indication_enabled="false" />
    <panel
     follows="all"
     height="400"
     layout="topleft"
     left="3"
     name="wearing_invpanel_placeholder" 
     top="0"
     visible="false"
     width="309" /> 
    <panel
     background_visible="true"
