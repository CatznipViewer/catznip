             left_delta="0"
             name="has media"
             top_pad="6"
             width="70" />
            <button
             follows="left|top"
             font="SansSerifSmall"
             height="20"
             label="Set Media Info"
             label_selected="Set Media Info"
             layout="topleft"
             left_pad="60"
             name="media info set"
             top_delta="-4"
