<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 follows="all"
 height="400"
 label="Search"
 layout="topleft"
 name="panel_places_search"
 width="600">
  <panel.string
   name="not_found">
      No results for &apos;[TEXT]&apos;
  </panel.string>
  <panel.string
   name="no_results">
      No results
  </panel.string>
  <panel.string
   name="count_results">
      Results [FIRST] - [LAST]
  </panel.string>
  <panel.string
   name="searching">
      Searching...
  </panel.string>
  <panel.string
   name="all_categories">
    All Categories
  </panel.string>

  <line_editor
   follows="top|left|right"
   height="23"
   layout="topleft"
   left="10"
   name="search_query"
   select_on_focus="true"
   top="5"
   width="275" />
  <combo_box
   height="23"
   layout="topleft"
   left_pad="5"
   name="search_category"
   width="140" />
  <button
   follows="top|right"
   height="23"
   label="Search"
   layout="topleft"
   left_pad="5"
   name="search_start"
   width="70" />

  <text
   follows="top|left"
   height="16"
   layout="topleft"
   left="10"
   name="search_maturity_label"
   top_pad="5"
   value="Maturity: "
   width="60" />
  <check_box
   height="16"
   label=""
   layout="topleft"
   left_pad="0"
   name="search_pg_check"
   top_delta="0"
   value="1"
   width="16" />
  <icon
   follows="top|left"
   height="16"
   image_name="Parcel_PG_Dark"
   layout="topleft"
   left_pad="5"
   name="search_pg_icon"
   top_delta="-1"
   width="18" />
  <check_box
   height="16"
   label=""
   layout="topleft"
   left_pad="10"
   name="search_mature_check"
   top_delta="1"
   value="1"
   width="16" />
  <icon
   follows="top|left"
   height="16"
   image_name="Parcel_M_Dark"
   layout="topleft"
   left_pad="5"
   name="search_mature_icon"
   top_delta="-1"
   width="18" />
  <check_box
   height="16"
   label=""
   layout="topleft"
   left_pad="10"
   name="search_adult_check"
   top_delta="1"
   value="1"
   width="16" />
  <icon
   follows="top|left"
   height="16"
   image_name="Parcel_R_Dark"
   layout="topleft"
   left_pad="5"
   name="search_adult_icon"
   top_delta="-1"
   width="18" />
   
  <scroll_list
   bottom="-38"
   draw_heading="true" 
   follows="all"
   layout="topleft"
   left="10"
   name="search_results"
   top="55"
   width="300">
    <columns
     label="Name"
     name="name" />
    <columns
     label="Traffic"
     name="traffic"
     width="100" />
  </scroll_list>
  <button
   bottom="-10"
   enabled="false"
   follows="bottom|left"
   height="23"
   image_bottom_pad="2"
   image_overlay="Arrow_Left_Off"
   image_overlay_alignment="left"
   label="Previous"
   layout="topleft"
   left="10"
   name="search_prev_btn"
   width="80" />
  <text
   follows="bottom|left|right"
   halign="center"
   height="22"
   layout="topleft"
   left_pad="5"
   name="search_results_label"
   top_delta="0"
   valign="center"
   width="130">
    Results 990-1000
  </text>
  <button
   enabled="false"
   follows="bottom|right"
   height="23"
   image_bottom_pad="2"
   image_overlay="Arrow_Right_Off"
   image_overlay_alignment="right"
   label="Next"
   layout="topleft"
   left_pad="5"
   name="search_next_btn"
   top_delta="0"
   width="80" />
  
  
  <panel
   bottom="-10"
   class="panel_parcel_info"
   filename="panel_parcel_info.xml"
   follows="top|right"
   layout="topleft"
   left="320"
   name="search_parcel"
   top="55"
   width="280" />

</panel>
