<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 follows="all"
 height="400"
 label="Search"
 layout="topleft"
 name="panel_places_search"
 width="600">
  <panel.string
   name="not_found">
      &apos;[TEXT]&apos; not found
  </panel.string>
  <panel.string
   name="no_results">
      No results
  </panel.string>
  <panel.string
   name="searching">
      Searching...
  </panel.string>

  <line_editor
   follows="top|left|right"
   height="23"
   layout="topleft"
   left="10"
   name="search_query"
   select_on_focus="true"
   top="5"
   width="275" />
  <button
   follows="top|right"
   height="23"
   label="Search"
   layout="topleft"
   left_pad="5"
   name="search_start"
   width="70" />
	 
  <scroll_list
   draw_heading="true" 
   follows="all"
   height="355"
   layout="topleft"
   left="10"
   name="search_results"
   top_pad="7"
   width="305">
    <columns
     label="Name"
     name="name"
     width="150" />
    <columns
     label="Traffic"
     name="traffic"
     width="150" />
  </scroll_list>
  <panel
   class="panel_parcel_info"
   filename="panel_parcel_info.xml"
   follows="top|right"
   height="355"
   layout="topleft"
   left_pad="10"
   name="search_parcel"
   top_delta="0"
   width="280" />

</panel>
