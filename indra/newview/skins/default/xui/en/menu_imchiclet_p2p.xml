<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<context_menu
 layout="topleft"
 name="IMChiclet P2P Menu">
    <menu_item_call
     label="View Profile"
     layout="topleft"
     name="Show Profile">
        <menu_item_call.on_click
         function="IMChicletMenu.Action"
         parameter="profile" />
    </menu_item_call>
    <menu_item_call
     label="Add Friend"
     layout="topleft"
     name="Add Friend">
        <menu_item_call.on_click
         function="IMChicletMenu.Action"
         parameter="add" />
    </menu_item_call>
    <menu_item_call
     label="Teleport"
     layout="topleft"
     name="Teleport">
        <menu_item_call.on_click
         function="IMChicletMenu.Action"
         parameter="teleport" />
    </menu_item_call>
    <menu_item_separator
     layout="topleft" />
    <menu_item_call
     label="Show Session"
     layout="topleft"
     name="Send IM">
        <menu_item_call.on_click
         function="IMChicletMenu.Action"
         parameter="show" />
    </menu_item_call>
    <menu_item_call
     label="End Session"
     layout="topleft"
     name="End Session">
        <menu_item_call.on_click
         function="IMChicletMenu.Action"
         parameter="end" />
    </menu_item_call>
</context_menu>
