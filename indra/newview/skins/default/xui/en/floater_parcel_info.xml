<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
 can_close="true"
 can_resize="false"
 height="400"
 layout="topleft"
 name="floater_parcel_info"
 positioning="cascading"
 save_rect="true"
 title="Parcel Information"
 width="300">
    <panel
     class="panel_parcel_info"
     filename="panel_parcel_info.xml"
     follows="all"
     height="364"
     layout="topleft"
     left="0"
     name="panel_parcel_info"
     top="5"
     width="300" />
    
    <panel
     background_visible="false"
     follows="left|right|bottom"
     height="25"
     label="bottom_panel"
     layout="topleft"
     left="5"
     name="bottom_panel"
     top_pad="0" >
        <icon  
         height="25"
         image_name="Toolbar_Left_Off"
         width="18"
         left="0"
         name="derender_spacer_icon"
         top="0" />
        <button
         height="25"
         image_hover_unselected="Toolbar_Middle_Over"
         image_overlay="Move_Run_Off"
         image_overlay_alignment="left"
         image_selected="Toolbar_Middle_Selected"
         image_unselected="Toolbar_Middle_Off"
         label="Teleport"
         left_pad="1"
         name="teleport_btn"
         tool_tip="Teleport to the selected area"
         width="115" />
        <icon  
         height="25"
         image_name="Toolbar_Middle_Off"
         width="20"
         left_pad="1"
         name="derender_spacer_icon"
         top="0" />
        <button
          follows="bottom|left"
          height="25"
          image_hover_unselected="Toolbar_Middle_Over"
          image_overlay="Command_Map_Icon"
          image_overlay_alignment="left"
          image_selected="Toolbar_Middle_Selected"
          image_unselected="Toolbar_Middle_Off"
          label="Show on Map"
          layout="topleft"
          left_pad="1"
          name="map_btn"
          tool_tip="Show the corresponding area on the World Map"
          width="115" />
        <icon  
         follows="bottom|right"
         height="25"
         image_name="Toolbar_Right_Off"
         width="18"
         left_pad="1"
         name="derender_spacer_icon"
         top="0" />
    </panel>
</floater>