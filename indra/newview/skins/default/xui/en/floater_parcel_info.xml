<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
 can_close="true"
 can_resize="false"
 height="390"
 layout="topleft"
 name="floater_parcel_info"
 positioning="cascading"
 save_rect="true"
 title="Parcel Information"
 width="300">
  <panel
   class="panel_parcel_info"
   filename="panel_parcel_info.xml"
   follows="all"
   height="350"
   layout="topleft"
   left="0"
   name="panel_parcel_info"
   top="5"
   width="300" />

  <button
   follows="bottom|left"
   height="20"
   label="Teleport"
   layout="topleft"
   left="15"
   name="teleport_btn"
   tool_tip="Teleport to the selected area"
   top_pad="5"
   width="125" />
  <button
   follows="bottom|left"
   height="20"
   label="Show on Map"
   layout="topleft"
   left_pad="20"
   name="map_btn"
   tool_tip="Show the corresponding area on the World Map"
   top_delta="0"
   width="125" />
</floater>
