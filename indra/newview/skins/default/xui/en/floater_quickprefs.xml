<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
 bg_alpha_image="Window_NoTitle_Background"
 bg_opaque_image="Window_NoTitle_Foreground"
 can_close="true"
 can_minimize="false"
 chrome="true"
 follows="top|left"
 header_height="0"
 height="330"
 layout="topleft"
 name="floater_quickprefs"
 positioning="specified"
 save_rect="true"
 save_visibility="true"
 title="QUICK PREFERENCES"
 width="310">

    <button
     follows="top|right"
     height="21"
     image_overlay="Command_Avatar_Icon"
     image_selected="PushButton_Selected_Press"
     label=""
     layout="topleft"
     right="-100"
     name="appearance_btn"
     tab_stop="false"
     tool_tip="Quick Appearance"
     width="25">
    </button>
    <button
     follows="top|right"
     height="21"
     image_overlay="Command_Appearance_Icon"
     image_selected="PushButton_Selected_Press"
     label=""
     layout="topleft"
     left_pad="1"
     name="wearing_btn"
     tab_stop="false"
     tool_tip="Worn Items"
     width="25">
    </button>
    <button
     follows="top|right"
     height="21"
     image_overlay="Command_Inventory_Icon"
     image_selected="PushButton_Selected_Press"
     label=""
     layout="topleft"
     left_pad="1"
     name="inventory_btn"
     tab_stop="false"
     tool_tip="Quick Inventory"
     width="25">
    </button>
    <button
     follows="top|right"
     height="21"
     image_overlay="Command_View_Icon"
     image_selected="PushButton_Selected_Press"
     label=""
     layout="topleft"
     left_pad="1"
     name="windlight_btn"
     tab_stop="false"
     tool_tip="Quick Environment"
     width="25">
    </button>

	<panel
	 border="false"
	 bottom="-1"
	 follows="all"
	 layout="topleft"
	 left="1"
	 top="25"
	 mouse_opaque="false"
	 name="controls"
	 right="-1">
		<panel
		 bottom="-2"
		 class="quickprefs_appearance"
		 filename="panel_quickprefs_appearance.xml"
		 follows="all"
		 layout="topleft"
		 left="4"
		 name="appearance_panel"
		 right="-4"
		 top="2"
		 visible="false" />
		<panel
		 bottom="-2"
		 class="quickprefs_wearing"
		 filename="panel_quickprefs_wearing.xml"
		 follows="all"
		 layout="topleft"
		 left="4"
		 name="wearing_panel"
		 right="-4"
		 top="2"
		 visible="false" />
		<panel
		 bottom="-2"
		 class="quickprefs_inventory"
		 filename="panel_quickprefs_inventory.xml"
		 follows="all"
		 layout="topleft"
		 left="4"
		 name="inventory_panel"
		 right="-4"
		 top="2"
		 visible="false" />
		<panel
		 bottom="-2"
		 class="quickprefs_windlight"
		 filename="panel_quickprefs_windlight.xml"
		 follows="all"
		 layout="topleft"
		 left="4"
		 name="windlight_panel"
		 right="-4"
		 top="2"
		 visible="false" />
	</panel>
</floater>
