<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<context_menu
 layout="topleft"
 name="IMChiclet Group Menu">
    <menu_item_call
     label="Group Info"
     layout="topleft"
     name="Show Profile">
        <menu_item_call.on_click
         function="IMChicletMenu.Action"
         parameter="profile" />
    </menu_item_call>
    <menu_item_separator
     layout="topleft" />
    <menu_item_call
     label="Show Session"
     layout="topleft"
     name="Chat">
        <menu_item_call.on_click
         function="IMChicletMenu.Action"
         parameter="show" />
    </menu_item_call>
    <menu_item_call
     label="End Session"
     layout="topleft"
     name="End Session">
        <menu_item_call.on_click
         function="IMChicletMenu.Action"
         parameter="end" />
    </menu_item_call>
</context_menu>
